framework module Quickly {
    umbrella header "Quickly.h"

    export *
    module * { export * }

    explicit module Private {
        header "QuicklyPrivate.h"
    }

}

module QuicklySqlite3IOS [system] {
    header "/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk/usr/include/sqlite3.h"
    link "sqlite3"
    export *
}


